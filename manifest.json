{
  "manifest_version": 2,

  "name": "Feed Notifier",
  "description": "Display popup notification on rss feed",
  "version": "1.0",

  "browser_action": {
    "default_icon": "./images/icon.png",
    "default_popup": "./templates/popup.html"
  },

  "options_page": "./templates/options.html",
  "permissions": [
    "storage"
  ]
}